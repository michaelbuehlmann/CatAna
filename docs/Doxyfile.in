PROJECT_NAME = CatAna
INPUT += @CMAKE_CURRENT_SOURCE_DIR@/../README.md
INPUT += @CMAKE_CURRENT_SOURCE_DIR@/
USE_MDFILE_AS_MAINPAGE = @CMAKE_CURRENT_SOURCE_DIR@/MAINPAGE.md

IMAGE_PATH=@CMAKE_CURRENT_SOURCE_DIR@

INPUT += @CMAKE_CURRENT_SOURCE_DIR@/../catana/include
INPUT += @CMAKE_CURRENT_SOURCE_DIR@/../python/CatAna

FILE_PATTERNS = *.cpp *.md *.hpp *.py
FILTER_PATTERNS = *.py=@CMAKE_CURRENT_SOURCE_DIR@/py_filter

BUILTIN_STL_SUPPORT = YES
RECURSIVE = YES

GENERATE_HTML = YES
GENERATE_LATEX = NO